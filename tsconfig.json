{
  "compilerOptions": {
    "outDir": "./dist/",
    "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      "*": [
        "src/*"
      ]
    },
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "strict": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react"
  }
}
